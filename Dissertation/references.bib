	issn = {0749-0208},
	journal = {Journal of Coastal Research},
	month = {04},
	number = {4},
	pages = {840-852},
	title = {{Tectonic Processes along the South America Coastline Derived from Quaternary Marine Terraces}},
	url = {https://doi.org/10.2112/JCOASTRES-D-14-00178.1},
