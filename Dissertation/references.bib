	doi = {https://doi.org/10.1016/S1040-6182(98)00031-7},
	issn = {1040-6182},
	journal = {Quaternary International},
	number = {1},
	pages = {101-113},
	title = {Interglacial sea levels},
	url = {https://www.sciencedirect.com/science/article/pii/S1040618298000317},
	volume = {55},
	year = {1999},
	bdsk-url-1 = {https://www.sciencedirect.com/science/article/pii/S1040618298000317},
