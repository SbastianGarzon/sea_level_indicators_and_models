	keywords = {Neotectonics, Wrench faults, Gulf of Guayaquil, Uplift, Oblique convergence, Pull-apart},
	note = {Andean Geodynamics:},
	number = {1},
	pages = {331-350},
	title = {Morphological and microtectonic analysis of Quaternary deformation from Pun{\'a} and Santa Clara Islands, Gulf of Guayaquil, Ecuador (South America)},
