	journal = {Quaternary International},
	note = {Quaternary Land-Ocean Interactions: Driving Mechanisms and Coastal Responses},
	number = {1},
	pages = {159-174},
	title = {Pleistocene marine calcareous macro-and-microfossils of Navarino Island (Chile) as environmental proxies during the last interglacial in southern South America},
