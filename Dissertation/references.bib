	publisher = {Klartext},
	series = {Essener geographische Arbeiten},
	title = {Jungk{\"a}nozoische Landschaftsgeschichte Patagoniens (Argentinien): andine Vorlandvergletscherungen, Talentwicklung und marine Terrassen},
	volume = {29},
